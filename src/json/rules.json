{
    "routing_rules": [
        {
            "match_genres": [
                "patriotic",
                "domoljubne"
            ],
            "target_path": "Cro/Domoljubne/{filename}"
        },
        {
            "match_genres": [
                "cro pop"
            ],
            "target_path": "Cro/{year}/{filename}"
        },
        {
            "match_genres": [
                "pop"
            ],
            "target_path": "{year}/{filename}"
        },
        {
            "match_genres": [
                "jazz",
                "religiozne",
                "oldies",
                "x-mas",
                "country",
                "slow",
                "metal",
                "navijacke",
                "rock",
                "dance",
                "trance",
                "electronic",
                "funk",
                "blues",
                "reggae"
            ],
            "target_path": "{genre}/{filename}"
        },
        {
            "match_genres": [
                "acoustic"
            ],
            "target_path": "akustika/{filename}"
        },
        {
            "match_genres": [
                "club"
            ],
            "target_path": "clubbing/{filename}"
        }
    ],
    "default_rule": "{genre}/{year}/{filename}"
}